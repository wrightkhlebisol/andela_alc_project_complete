  },
  "author": "Caleb Ogundiya",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^8.0.0",
    "browser-request": "^0.3.3",
    "browserify": "^14.5.0",
    "http-server": "^0.10.0",
    "tiny-emitter": "^1.0.2"
  }
}
